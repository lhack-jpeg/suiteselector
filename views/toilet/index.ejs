<% layout('layouts/boilerplate') %>
<h1 class="title">Toilets</h1>
<div class="container">
    <% for (let toilet of toilets) { %>
    <div class="card">
        <h3 class="card-title"><%= toilet.name%></h3>
        <img class="card-image" src="<%= toilet.image%>" alt="" />
        <a class="card-button" href="/show/<%=toilet.code%>">See toilet</a>
    </div>
    <% } %>
</div>
