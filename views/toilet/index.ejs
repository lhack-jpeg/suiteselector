<% layout('layouts/boilerplate') %>
<h1>All Toilets</h1>
<div class="container">
    <% for (let toilet of toilets) { %>
    <div class="card">
        <h3><%= toilet.name%></h3>
        <img src="<%= toilet.image%>" alt="" />
        <a href="/show/<%=toilet.code%>">See toilet</a>
    </div>
    <% } %>
</div>
